# Homepage Environment Variables
# Copy this file to docker.env and fill in your actual values

# Your Pi-hole password
HOMEPAGE_VAR_PIHOLE_KEY='your_pihole_password'

# Your *arr API Keys
HOMEPAGE_VAR_RADARR_KEY=your_radarr_api_key
HOMEPAGE_VAR_SONARR_KEY=your_sonarr_api_key
HOMEPAGE_VAR_PROWLARR_KEY=your_prowlarr_api_key
HOMEPAGE_VAR_LIDARR_KEY=your_lidarr_api_key
HOMEPAGE_VAR_BAZARR_KEY=your_bazarr_api_key
HOMEPAGE_VAR_JACKETT_KEY=your_jackett_password

# Your qBittorrent Credentials
HOMEPAGE_VAR_QBITTORRENT_USER=your_qbittorrent_username
HOMEPAGE_VAR_QBITTORRENT_PASS=your_qbittorrent_password

# Your Jellyfin Stack API Keys
HOMEPAGE_VAR_JELLYFIN_KEY=your_jellyfin_api_key
HOMEPAGE_VAR_JELLYSEERR_KEY=your_jellyseerr_api_key
HOMEPAGE_VAR_JELLYSTAT_KEY=your_jellystat_api_key

# Your Tailscale API Key
HOMEPAGE_VAR_TAILSCALE_KEY=your_tailscale_api_key

# Your Immich API Key
HOMEPAGE_VAR_IMMICH_KEY=your_immich_api_key

# Your Nextcloud API Key (NC-Token)
HOMEPAGE_VAR_NEXTCLOUD_KEY=your_nextcloud_api_key

# Your Nginx Proxy Manager Credentials
HOMEPAGE_VAR_NPM_USER=your_npm_email
HOMEPAGE_VAR_NPM_PASS=your_npm_password

# Your Cloudflare Tunnels API Key
HOMEPAGE_VAR_CLOUDFLARE_KEY=your_cloudflare_api_key

# Your Beszel Credentials
HOMEPAGE_VAR_BESZEL_USER=your_beszel_email
HOMEPAGE_VAR_BESZEL_PASS='your_beszel_password'

# Soulseek API Key
HOMEPAGE_VAR_SLSKD_KEY=your_slskd_api_key