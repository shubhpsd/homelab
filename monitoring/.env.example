# Monitoring Environment Variables
# Copy this file to .env and fill in your values

# Beszel Hub Connection
BESZEL_HUB_URL=http://YOUR_HUB_IP:8090

# Agent Authentication (get these from Beszel Hub web interface)
BESZEL_AGENT_KEY=ssh-ed25519_your_public_key_here
BESZEL_AGENT_TOKEN=your_agent_token_here